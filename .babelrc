{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        ["@babel/preset-env", {
            "targets": {
                "chrome": "70",
                "ie": "11",
                "safari": "10",
                "firefox": "60"
            }
        }]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-transform-runtime"
    ]
}
